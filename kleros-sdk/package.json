    "release:major": "scripts/publish.sh major"
  },
  "devDependencies": {
    "@types/mustache": "^4.2.5",
    "@types/node": "^20.17.6",
    "@vitest/ui": "^1.6.0",
    "mocha": "^10.8.2",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3",
    "viem": "^2.21.48",
    "vitest": "^1.6.0"
  },
  "dependencies": {
    "@reality.eth/reality-eth-lib": "^3.2.44",
    "@urql/core": "^5.0.8",
    "mustache": "^4.2.0",
    "zod": "^3.23.8"
  },
