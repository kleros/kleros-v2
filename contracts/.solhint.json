{
  "extends": "solhint:recommended",
  "plugins": ["prettier"],
  "rules": {
    "compiler-version": ["error", "^0.8.0"],
    "func-visibility": [
      "warn",
      {
        "ignoreConstructors": true
      }
    ],
    "prettier/prettier": "warn",
    "max-line-length": "off",
    "check-send-result": "off",
    "not-rely-on-time": "off",
    "multiple-sends": "off",
    "quotes": "warn"
  }
}
